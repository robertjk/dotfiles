# zshrc

# Source parts of the ZSH configuration

# Zim initialization must be first, as other things might depend on it
source "$HOME/.config/zsh/zim-init.zsh"

source "$HOME/.config/zsh/empty-line-between-prompts.zsh"
source "$HOME/.config/zsh/history-file.zsh"
source "$HOME/.config/zsh/man-pages-colors.zsh"
source "$HOME/.config/zsh/aliases.zsh"
source "$HOME/.config/zsh/fnm.zsh"
source "$HOME/.config/zsh/fzf.zsh"
